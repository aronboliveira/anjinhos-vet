{
  "name": "anjinhos-landing-page",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "ssb": "sass src/styles/sass:src/styles",
    "sswt": "sass --watch --embed-source-map src/styles/sass:src/styles",
    "wt--all": "run-p sswt dev",
    "wbuild": "tsc -b && npm run sbuild && webpack --config webpack.config.cjs --mode production"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "typescript": "^5.6.2",
    "vue": "^3.4.37",
    "vue-tsc": "^2.0.29",
    "vite": "^5.4.1",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-vue": "^5.1.2"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.10.0",
    "eslint-plugin-vue": "^9.28.0",
    "html-replace-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "sass": "^1.78.0",
    "sass-loader": "^16.0.1",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-subresource-integrity": "^5.2.0-rc.1"
  }
}
